{
  "name": "jupyterlab-search-replace",
  "version": "1.0.2",
  "description": "Search and replace across files",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab-contrib/search-replace.git",
  "bugs": {
    "url": "https://github.com/jupyterlab-contrib/search-replace.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Frederic Collonval",
    "email": "fcollonval@gmail.com"
  },
  "contributors": [
    {
      "name": "Mariana Meireles"
    },
    {
      "name": "Madhur Tandon"
    }
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab-contrib/search-replace.git.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_search_replace/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyter-notebook/react-components": "^0.9.1",
    "@jupyterlab/application": "^3.1.9",
    "@jupyterlab/apputils": "^3.1.9",
    "@jupyterlab/coreutils": "^5.1.9",
    "@jupyterlab/docmanager": "^3.1.9",
    "@jupyterlab/filebrowser": "^3.1.9",
    "@jupyterlab/fileeditor": "^3.1.9",
    "@jupyterlab/services": "^6.1.9",
    "@jupyterlab/settingregistry": "^3.1.9",
    "@jupyterlab/translation": "^3.1.9",
    "@jupyterlab/ui-components": "^3.1.9",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/messaging": "^1.7.2",
    "@lumino/widgets": "^1.26.2",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@jupyterlab/codemirror": "^3.1.9",
    "@jupyterlab/docregistry": "^3.1.9",
    "@types/codemirror": "^0.0.109",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vscode/codicons": "^0.0.29",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "resolutions": {
    "@jupyterlab/rendermime-interfaces": ">=3.0.0 <3.8.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_search_replace"
        }
      }
    },
    "extension": true,
    "outputDir": "jupyterlab_search_replace/labextension",
    "schemaDir": "schema",    
    "sharedPackages": {
      "@jupyter-notebook/react-components": {
        "bundled": true,
        "singleton": true
      },
      "@jupyter-notebook/web-components": {
        "bundled": true,
        "singleton": true
      },
      "@microsoft/fast-element": {
        "bundled": true,
        "singleton": true
      },
      "@microsoft/fast-foundation": {
        "bundled": true,
        "singleton": true
      }
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
